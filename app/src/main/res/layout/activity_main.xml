<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/textViewredbottom"
        android:layout_width="736dp"
        android:layout_height="54dp"
        android:background="@color/redbackground"
        tools:layout_editor_absoluteY="690dp" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="136dp"
        android:layout_height="137dp"
        android:src="@mipmap/ic_launcher_foreground"
        app:layout_constraintBottom_toTopOf="@+id/guideline4"
        app:layout_constraintEnd_toStartOf="@+id/imageView2"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/guideline"
        app:layout_constraintTop_toBottomOf="@+id/guideline3"
        tools:srcCompat="@mipmap/ic_launcher_foreground" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="60dp"
        android:layout_height="47dp"
        android:fontFamily="sans-serif-light"
        android:text="I'm"
        android:textAlignment="center"
        android:textSize="30sp"
        tools:layout_editor_absoluteX="223dp"
        tools:layout_editor_absoluteY="227dp" />


    <TextView
        android:id="@+id/myNameTextView"
        android:layout_width="144dp"
        android:layout_height="46dp"
        android:text="Robert Jr."
        android:textColor="@color/boldtext"
        android:textSize="30sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="291dp"
        tools:layout_editor_absoluteY="227dp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="115dp"
        android:layout_height="69dp"
        app:srcCompat="@mipmap/asset_textbox_red_new_foreground"
        tools:layout_editor_absoluteX="201dp"
        tools:layout_editor_absoluteY="141dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="38dp"
        android:layout_height="20dp"
        android:text="Hi"
        android:textAlignment="center"
        android:textColor="@color/boldtext"
        tools:layout_editor_absoluteX="244dp"
        tools:layout_editor_absoluteY="165dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="48dp"
        android:layout_height="21dp"
        android:text="Age"
        android:textColor="@color/boldtext"
        tools:layout_editor_absoluteX="101dp"
        tools:layout_editor_absoluteY="320dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="56dp"
        android:layout_height="20dp"
        android:text="Address"
        android:textColor="@color/boldtext"
        tools:layout_editor_absoluteX="97dp"
        tools:layout_editor_absoluteY="353dp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Email"
        android:textColor="@color/boldtext"
        tools:layout_editor_absoluteX="107dp"
        tools:layout_editor_absoluteY="386dp" />

    <android.support.constraint.Barrier
        android:id="@+id/barrier4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:barrierDirection="left" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif"
        android:text="Phone"
        android:textColor="@color/boldtext"
        tools:layout_editor_absoluteX="104dp"
        tools:layout_editor_absoluteY="417dp" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Freelance"
        android:textColor="@color/boldtext"
        tools:layout_editor_absoluteX="93dp"
        tools:layout_editor_absoluteY="447dp" />

    <TextView
        android:id="@+id/addressTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="55 Iron Dr., Los Angeles, California"
        tools:layout_editor_absoluteX="229dp"
        tools:layout_editor_absoluteY="352dp" />

    <TextView
        android:id="@+id/emailTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="rdj@testemail.com"
        tools:layout_editor_absoluteX="274dp"
        tools:layout_editor_absoluteY="384dp" />

    <TextView
        android:id="@+id/phoneTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="(+1) 800-555-1234"
        tools:layout_editor_absoluteX="276dp"
        tools:layout_editor_absoluteY="415dp" />

    <TextView
        android:id="@+id/freeLanceTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="till April 15"
        tools:layout_editor_absoluteX="303dp"
        tools:layout_editor_absoluteY="447dp" />

    <TextView
        android:id="@+id/TextViewunderlinebar"
        android:layout_width="180dp"
        android:layout_height="2dp"
        android:background="@color/greyline"
        android:textColor="@color/greyline"
        tools:layout_editor_absoluteX="254dp"
        tools:layout_editor_absoluteY="272dp" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="39dp"
        android:layout_height="35dp"
        android:src="@drawable/ic_icon_social_facebook"
        tools:layout_editor_absoluteX="60dp"
        tools:layout_editor_absoluteY="700dp" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="34dp"
        android:layout_height="37dp"
        android:src="@drawable/ic_icon_social_twitter"
        tools:layout_editor_absoluteX="138dp"
        tools:layout_editor_absoluteY="699dp" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="35dp"
        android:layout_height="39dp"
        android:src="@drawable/ic_icon_social_linkedin"
        tools:layout_editor_absoluteX="231dp"
        tools:layout_editor_absoluteY="698dp" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="42dp"
        android:layout_height="37dp"
        android:src="@drawable/ic_googleplus"
        tools:layout_editor_absoluteX="305dp"
        tools:layout_editor_absoluteY="699dp" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="38dp"
        android:layout_height="35dp"
        app:srcCompat="@drawable/ic_language_black_24dp"
        tools:layout_editor_absoluteX="376dp"
        tools:layout_editor_absoluteY="700dp" />

    <TextView
        android:id="@+id/ageTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="53"
        tools:layout_editor_absoluteX="328dp"
        tools:layout_editor_absoluteY="321dp" />

    <android.support.constraint.Guideline
        android:id="@+id/guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.08" />

    <android.support.constraint.Guideline
        android:id="@+id/guideline2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent=".44" />

    <android.support.constraint.Guideline
        android:id="@+id/guideline3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.06" />

    <android.support.constraint.Guideline
        android:id="@+id/guideline4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.36" />

    <android.support.constraint.Guideline
        android:id="@+id/guideline5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.56" />

    <android.support.constraint.Guideline
        android:id="@+id/guideline6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent=".96" />

    <android.support.constraint.Guideline
        android:id="@+id/guideline7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent=".80" />

    <android.support.constraint.Guideline
        android:id="@+id/guideline8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent=".88" />

    <Button
        android:id="@+id/buttonlogout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Logout"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/guideline8"
        tools:text="Exit" />


</android.support.constraint.ConstraintLayout>